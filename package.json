{
  "name": "react-todo",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node ./dist/src/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "dev:studio": "npx drizzle-kit studio",
    "migrate:gen": "npx drizzle-kit generate",
    "migrate:push": "npx drizzle-kit push",
    "migrate:up": "npx drizzle-kit up",
    "test": "cross-env NODE_ENV=test vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.3",
    "@hono/zod-openapi": "^0.16.4",
    "@libsql/client": "^0.14.0",
    "@scalar/hono-api-reference": "^0.5.156",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.36.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.8",
    "hono-pino": "^0.4.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^20.11.17",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.27.0",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.7.1",
    "typescript-eslint": "^8.12.2",
    "vitest": "^2.1.5"
  }
}